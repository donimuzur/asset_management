<!DOCTYPE html>
<html lang="en" class="no-focus">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">

        <title>Asset Management System</title>

        <meta name="description" content="Asset Management System">
        <meta name="author" content="zulfi.rusdani@polowijogosari.com">
        <meta name="robots" content="noindex, nofollow">

        <!-- Icons -->
        <link rel="shortcut icon" href="/assets/media/favicons/favicon.png">
        <link rel="icon" type="image/png" sizes="192x192" href="/assets/media/favicons/favicon-192x192.png">
        <link rel="apple-touch-icon" sizes="180x180" href="/assets/media/favicons/apple-touch-icon-180x180.png">
        <!-- END Icons -->

        <!-- Stylesheets -->
        <link rel="stylesheet" href="/assets/js/plugins/sweetalert2/sweetalert2.min.css">
        <link rel="stylesheet" href="/assets/js/plugins/datatables/dataTables.bootstrap4.css">
        <link rel="stylesheet" href="/assets/js/plugins/datatables/rowGroup.dataTables.min.css">
        <link rel="stylesheet" href="/assets/js/plugins/easyautocomplete/easy-autocomplete.min.css" >

        {% block steylesheets %}
        
        {% endblock %}
        <!-- Fonts and Codebase framework -->
        <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Muli:300,400,400i,600,700">
        <link rel="stylesheet" id="css-main" href="/assets/css/codebase.min.css">

        <!-- END Stylesheets -->
    </head>
    <body>
        <!-- Page Container -->
        {% block page_container %}
        <div id="page-container" class="sidebar-o sidebar-inverse enable-page-overlay side-scroll page-header-fixed main-content-narrow">
        {% endblock %}
            <!-- Sidebar -->
            <nav id="sidebar">
                <!-- Sidebar Content -->
                <div class="sidebar-content">
                    <!-- Side Header -->
                    <div class="content-header content-header-fullrow px-15">
                        <!-- Normal Mode -->
                        <div class="content-header-section text-center align-parent sidebar-mini-hidden">
                            <!-- Close Sidebar, Visible only on mobile screens -->
                            <!-- Layout API, functionality initialized in Template._uiApiLayout() -->
                            <button type="button" class="btn btn-circle btn-dual-secondary d-lg-none align-v-r" data-toggle="layout" data-action="sidebar_close">
                                <i class="fa fa-times text-danger"></i>
                            </button>
                            <!-- END Close Sidebar -->

                            <!-- Logo -->
                            <div class="content-header-item">
                                <a class="link-effect font-w700" href="{{ path('default') }}">
                                    <span class="font-size-xl text-dual-primary-dark">Asset </span><span class="font-size-xl text-primary">Management</span>
                                </a>
                            </div>
                            <!-- END Logo -->
                        </div>
                        <!-- END Normal Mode -->
                    </div>
                    <!-- END Side Header -->

                    <!-- Side User -->
                    <div class="content-side content-side-full content-side-user px-10 align-parent">
                        <!-- Visible only in normal mode -->
                        <div class="sidebar-mini-hidden-b text-center">
                            <a class="img-link" href="">
                                <img class="img-avatar" src="/assets/media/avatars/avatar15.jpg" alt="">
                            </a>
                            <ul class="list-inline mt-10">
                                <li class="list-inline-item">
                                    <a class="link-effect text-dual-primary-dark font-size-xs font-w600 text-uppercase">{{app.user.FullName}}</a>
                                </li>
                                <li class="list-inline-item">
                                    <!-- Layout API, functionality initialized in Template._uiApiLayout() -->
                                    <a class="link-effect text-dual-primary-dark" data-toggle="layout" data-action="sidebar_style_inverse_toggle" href="javascript:void(0)">
                                        <i class="si si-drop"></i>
                                    </a>
                                </li>
                                <li class="list-inline-item">
                                    <a class="link-effect text-dual-primary-dark" href="{{ path('app_logout')}}">
                                        <i class="si si-logout"></i>
                                    </a>
                                </li>
                            </ul>
                        </div>
                        <!-- END Visible only in normal mode -->
                    </div>
                    <!-- END Side User -->

                    <!-- Side Navigation -->
                    <div class="content-side content-side-full">
                        <ul class="nav-main">
                            <li>
                                <a class="{% if app.request.get('_route') == 'default' %}active{% endif %}" href="{{ path('default') }}"><i class="si si-grid"></i><span class="sidebar-mini-hide">Dashboard</span></a>
                            </li>
                            <li class="{% if app.request.get('_route') starts with 'asset_tanah_' %}open{% endif %}">
                                <a class="nav-submenu" data-toggle="nav-submenu" href="#"><i class="si si-globe"></i><span class="sidebar-mini-hide">Asset Tanah</span></a>
                                <ul>
                                    <li>
                                        <a class="{% if app.request.get('_route') starts with 'asset_tanah_perusahaan_' %}active{% endif %}" href="{{ path('asset_tanah_perusahaan_index')}}"><span class="sidebar-mini-hide">Perusahaan</span></a>
                                    </li>
                                    <li>
                                        <a class="{% if app.request.get('_route') starts with 'asset_tanah_pribadi_' %}active{% endif %}" href="{{ path('asset_tanah_pribadi_index')}}"><span class="sidebar-mini-hide">Pribadi</span></a>
                                    </li>
                                </ul>
                            </li>
                            <li class="">
                                <a class="nav-submenu" data-toggle="nav-submenu" href="#"><i class="si si-home"></i><span class="sidebar-mini-hide">Asset Bangunan</span></a>
                                <ul>
                                    <li>
                                        <a class="" href=""><span class="sidebar-mini-hide">Perusahaan</span></a>
                                    </li>
                                    <li>
                                        <a class="" href=""><span class="sidebar-mini-hide">Pribadi</span></a>
                                    </li>
                                </ul>
                            </li>
                            <li class="{% if app.request.get('_route') starts with 'asset_kendaraan_' %}open{% endif %}">
                                <a class="nav-submenu" data-toggle="nav-submenu" href="#"><i class="si si-shuffle"></i><span class="sidebar-mini-hide">Asset Kendaraan</span></a>
                                <ul>
                                    <li>
                                        <a class="{% if app.request.get('_route') starts with 'asset_kendaraan_mobil_' %}active{% endif %}" href="{{ path('asset_kendaraan_mobil_index')}}"><span class="sidebar-mini-hide">Mobil</span></a>
                                    </li>
                                    <li>
                                        <a class="{% if app.request.get('_route') starts with 'asset_kendaraan_motor_' %}active{% endif %}" href="{{ path('asset_kendaraan_motor_index')}}"><span class="sidebar-mini-hide">Motor</span></a>
                                    </li>
                                </ul>
                            </li>
                            {% if app.user.UserRoleId.Name == 'ROLE_ADMIN' %}
                            <li class="{% if app.request.get('_route') starts with 'master' %}open{% endif %}">
                                <a class="nav-submenu" data-toggle="nav-submenu" href="#"><i class="si si-settings"></i><span class="sidebar-mini-hide">Master</span></a>
                                <ul>
                                    <li>
                                        <a class="{% if app.request.get('_route') in ['master_asset_user_index','master_asset_user_new','master_asset_user_show','master_asset_user_edit'] %}active{% endif %}" href="{{ path('master_asset_user_index')}}"><span class="sidebar-mini-hide">Master User</span></a>
                                    </li>
                                    <li>
                                        <a class="{% if app.request.get('_route') in ['master_asset_user_role_index','master_asset_user_role_new','master_asset_user_role_show','master_asset_user_role_edit'] %}active{% endif %}" href="{{ path('master_asset_user_role_index')}}"><span class="sidebar-mini-hide">Master User Role</span></a>
                                    </li>
                                </ul>
                            </li>
                            {% endif %}
                        </ul>
                    </div>
                    <!-- END Side Navigation -->
                </div>
                <!-- Sidebar Content -->
            </nav>
            <!-- END Sidebar -->

            <!-- Header -->
            <header id="page-header">
                <!-- Header Content -->
                <div class="content-header">
                    <!-- Left Section -->
                    <div class="content-header-section">
                        <!-- Toggle Sidebar -->
                        <button type="button" class="btn btn-circle btn-dual-secondary" data-toggle="layout" data-action="sidebar_toggle">
                            <i class="fa fa-navicon"></i>
                        </button>
                        <!-- END Toggle Sidebar -->
                    </div>
                    <!-- END Left Section -->

                    <!-- Right Section -->
                    <div class="content-header-section">
                        <!-- User Dropdown -->
                        <div class="btn-group" role="group">
                            <button type="button" class="btn btn-rounded btn-dual-secondary" id="page-header-user-dropdown" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <i class="fa fa-user d-sm-none"></i>
                                <span class="d-none d-sm-inline-block">{{app.user.FullName}}</span>
                                <i class="fa fa-angle-down ml-5"></i>
                            </button>
                            <div class="dropdown-menu dropdown-menu-right min-width-200" aria-labelledby="page-header-user-dropdown">
                                <h5 class="h6 text-center py-10 mb-5 border-b text-uppercase">{{app.user.UserRoleId.DisplayName}}</h5>
                                <a class="dropdown-item" href="">
                                    <i class="si si-lock mr-5"></i> Change Password
                                </a>
                                <a class="dropdown-item" href="{{ path('app_logout')}}">
                                    <i class="si si-logout mr-5"></i> Sign Out
                                </a>
                            </div>
                        </div>
                        <!-- END User Dropdown -->
                    </div>
                    <!-- END Right Section -->
                </div>
                <!-- END Header Content -->

                <!-- Header Loader -->
                <div id="page-header-loader" class="overlay-header bg-primary">
                    <div class="content-header content-header-fullrow text-center">
                        <div class="content-header-item">
                            <i class="fa fa-sun-o fa-spin text-white"></i>
                        </div>
                    </div>
                </div>
                <!-- END Header Loader -->
            </header>
            <!-- END Header -->

            <!-- Main Container -->
            <main id="main-container">

                {% block body %}

                {% endblock %}

            </main>
            <!-- END Main Container -->

            <!-- Footer -->
            <footer id="page-footer" class="opacity-0">
                <div class="content py-20 font-size-xs clearfix">
                    <div class="float-left">
                        <a class="font-w600">Copyright Polowijo Gosari Indonesia</a> &copy; <span class="js-year-copy"></span>
                    </div>
                </div>
            </footer>
            <!-- END Footer -->
        </div>
        <!-- END Page Container -->        
        <script src="/assets/js/codebase.core.min.js"></script>
        <script src="/assets/js/codebase.app.min.js"></script>


        <!-- Page JS Plugins -->
        <script src="/assets/js/plugins/datatables/jquery.dataTables.min.js"></script>
        <script src="/assets/js/plugins/datatables/dataTables.rowGroup.min.js"></script>
        <script src="/assets/js/plugins/datatables/dataTables.bootstrap4.min.js"></script>
        <script src="/assets/js/plugins/bootstrap-notify/bootstrap-notify.min.js"></script>
        <script src="/assets/js/plugins/es6-promise/es6-promise.auto.min.js"></script>
        <script src="/assets/js/plugins/sweetalert2/sweetalert2.min.js"></script>        
        <script src="/assets/js/plugins/jquery-validation/jquery.validate.min.js"></script>
        <script src="/assets/js/plugins/jquery-validation/additional-methods.js"></script>
        <script src="/assets/js/plugins/easyautocomplete/jquery.easy-autocomplete.min.js"></script>


        {% block javascripts %}
        <script>

        function ShowHeaderLoader()
        {
            $('#page-header-loader').addClass("show");
        }
        function HideHeaderLoader()
        {
            $('#page-header-loader').removeClass("show");
        }
        var e = Swal.mixin({ buttonsStyling: !1, customClass: { confirmButton: "btn btn-alt-success m-5", cancelButton: "btn btn-alt-danger m-5"} });
        $(".js-swal-confirm").on("click", function (n) {
            let $form = $(this).closest('form');
            e.fire({
                title: "Are you sure?",
                type: "warning",
                showCancelButton: !0,
                customClass: { confirmButton: "btn btn-alt-danger m-1", cancelButton: "btn btn-alt-secondary m-1" },
                confirmButtonText: "Yes, delete it!",
                html: !1,
            }).then(function (n) {
                n.value ? $form.submit() : "cancel" === n.dismiss;
            });
        });
        
        setTimeout(function() {
            $('.alert').fadeOut('fast');
        }, 3000); 
        </script>
        {% endblock %}
    </body>
</html>
