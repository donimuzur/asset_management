<!DOCTYPE html>
<html lang="en">
    <head>
    <!-- Required meta tags -->
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <meta name="description" content="Asset Management System">
        <meta name="author" content="Polowijo Gosari Indonesia">

        <title>{% block title %}Welcome!{% endblock %}</title>

        <!-- vendor css -->
        <link href="/content/lib/font-awesome/css/font-awesome.css" rel="stylesheet">
        <link href="/content/lib/Ionicons/css/ionicons.css" rel="stylesheet">
        <link href="/content/lib/perfect-scrollbar/css/perfect-scrollbar.css" rel="stylesheet">
        <link href="/content/lib/jquery-toggles/toggles-full.css" rel="stylesheet">
        <link href="/content/lib/rickshaw/rickshaw.min.css" rel="stylesheet">


        <!-- Amanda CSS -->
        <link rel="stylesheet" href="/content/css/amanda.css">

        
    </head>

    <body>
        <div class="am-header">
            <div class="am-header-left">
                <a id="naviconLeft" href="" class="am-navicon d-none d-lg-flex"><i class="icon ion-navicon-round"></i></a>
                <a id="naviconLeftMobile" href="" class="am-navicon d-lg-none"><i class="icon ion-navicon-round"></i></a>
                <a href="index.html" class="am-logo">Asset Management System</a>
            </div><!-- am-header-left -->

            <div class="am-header-right">
                
                <div class="dropdown dropdown-profile">
                    <a href="" class="nav-link nav-link-profile" data-toggle="dropdown">
                        <img src="/content/img/img3.jpg" class="wd-32 rounded-circle" alt="">
                        <span class="logged-name"><span class="hidden-xs-down">{{app.user.FullName}}</span> <i class="fa fa-angle-down mg-l-3"></i></span>
                    </a>
                    <div class="dropdown-menu wd-200">
                        <ul class="list-unstyled user-profile-nav">
                            <li><a href="{{ path('app_logout')}}"><i class="icon ion-power"></i> Sign Out</a></li>
                        </ul>
                    </div><!-- dropdown-menu -->
                </div><!-- dropdown -->
            </div><!-- am-header-right -->
        </div><!-- am-header -->

        <div class="am-sideleft">
            <div class="tab-content">
                <div id="mainMenu" class="tab-pane active">
                    <ul class="nav am-sideleft-menu">
                        <li class="nav-item">
                            <a href="{{ path('default') }}" class="nav-link {% if app.request.get('_route') == 'default' %}active{% endif %}">
                                <i class="icon ion-grid"></i>
                                <span>Dashboard</span>
                            </a>
                        </li><!-- nav-item -->
                         <li class="nav-item">
                            <a href="" class="nav-link with-sub">
                                <i class="icon ion-earth"></i>
                                <span>Asset Tanah</span>
                            </a>
                            <ul class="nav-sub">
                                <li class="nav-item"><a href="#" class="nav-link ">Asset Perusahaan</a></li>
                                <li class="nav-item"><a href="#" class="nav-link ">Asset Pribadi</a></li>
                            </ul>
                        </li><!-- nav-item -->
                        <li class="nav-item">
                            <a href="" class="nav-link with-sub">
                                 <i class="icon ion-ios-home"></i>
                                <span>Asset Bangunan</span>
                            </a>
                            <ul class="nav-sub">
                                <li class="nav-item"><a href="#" class="nav-link ">Asset Perusahaan</a></li>
                                <li class="nav-item"><a href="#" class="nav-link">Asset Pribadi</a></li>
                            </ul>
                        </li><!-- nav-item -->
                        <li class="nav-item">
                            <a href="" class="nav-link with-sub {% if app.request.get('_route') starts with 'asset_kendaraan_' %}show-sub active{% endif %}">
                                 <i class="icon ion-android-car"></i>
                                <span>Asset Kendaraan</span>
                            </a>
                            <ul class="nav-sub">
                                <li class="nav-item"><a href="{{ path('asset_kendaraan_motor_index')}}" class="nav-link {% if app.request.get('_route') starts with 'asset_kendaraan_motor_' %}active{% endif %}">Motor</a></li>
                                <li class="nav-item"><a href="#" class="nav-link">Mobil</a></li>
                            </ul>
                        </li><!-- nav-item -->
                         {% if app.user.UserRoleId.Name == 'ROLE_ADMIN' %}
                        <li class="nav-item">
                            <a href="" class="nav-link with-sub {% if app.request.get('_route') starts with 'master' %}show-sub active{% endif %}">
                                 <i class="icon ion-gear-b"></i>
                                <span>Master</span>
                            </a>
                            <ul class="nav-sub">
                                <li class="nav-item"><a href="{{ path('master_asset_user_index')}}" class="nav-link {% if app.request.get('_route') in ['master_asset_user_index','master_asset_user_new','master_asset_user_show','master_asset_user_edit'] %}active{% endif %}">Master User</a></li>
                                <li class="nav-item"><a href="{{ path('master_asset_user_role_index')}}" class="nav-link {% if app.request.get('_route') in ['master_asset_user_role_index','master_asset_user_role_new','master_asset_user_role_show','master_asset_user_role_edit'] %}active{% endif %}">Master User Role</a></li>
                            </ul>
                        </li><!-- nav-item -->
                        {% endif %}
                    </ul>
                </div><!-- #mainMenu -->
            </div><!-- tab-content -->
        </div><!-- am-sideleft -->
        
        <div class="am-mainpanel">
            <div class="am-pagetitle">
                <h5 class="am-title">{% block HeaderTitle %}{% endblock %}</h5>
           
            </div><!-- am-pagetitle -->
            {% block body %}
            {% endblock %}
            <div class="am-footer">
                <span>Copyright &copy;. All Rights Reserved. Polowijo Gosari Indonesia.</span>
            </div><!-- am-footer -->
        </div><!-- am-mainpanel -->
        {% block javascripts %}
        <script src="/content/lib/jquery/jquery.js"></script>
        <script src="/content/lib/popper.js/popper.js"></script>
        <script src="/content/lib/bootstrap/bootstrap.js"></script>
        <script src="/content/lib/perfect-scrollbar/js/perfect-scrollbar.jquery.js"></script>
        <script src="/content/lib/jquery-toggles/toggles.min.js"></script>
        <script src="/content/lib/d3/d3.js"></script>
        <script src="/content/lib/rickshaw/rickshaw.min.js"></script>
        <script src="/content/lib/gmaps/gmaps.js"></script>
        <script src="/content/lib/Flot/jquery.flot.js"></script>
        <script src="/content/lib/Flot/jquery.flot.pie.js"></script>
        <script src="/content/lib/Flot/jquery.flot.resize.js"></script>
        <script src="/content/lib/flot-spline/jquery.flot.spline.js"></script>

        <script src="/content/lib/highlightjs/highlight.pack.js"></script>
        <script src="/content/lib/select2/js/select2.min.js"></script>
        <script src="/content/lib/parsleyjs/parsley.js"></script>
        <script src="/content/js/amanda.js"></script>
        <script src="/content/js/ResizeSensor.js"></script>
        <script src="/content/js/dashboard.js"></script>
        <script>
             setTimeout(function() {
                $('.alert').fadeOut('fast');
            }, 3000); 
        </script>
        {% endblock %}
    </body>
</html>
